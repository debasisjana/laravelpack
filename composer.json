{
    "name": "askedio/laravel-vendor-package",
    "description": "A Vendor Package Example",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.*"
     },
     "require-dev": {
        "laravel/laravel": "5.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Askedio\\LaravelVendorPackage\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Askedio\\LaravelVendorPackage\\Tests\\" : "tests"
        }
    }
}